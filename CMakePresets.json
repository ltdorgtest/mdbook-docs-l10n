{
  "version" : 4,
  "cmakeMinimumRequired" : {
    "major" : 3,
    "minor" : 25,
    "patch" : 0
  },
  "configurePresets" : [
    {
      "name" : "default",
      "hidden" : true,
      "binaryDir" : "${sourceDir}/out/build/${presetName}"
    },
    {
      "name" : "all",
      "displayName" : "All Languages",
      "description" : "All Languages",
      "inherits" : "default",
      "cacheVariables" : {
        "LANGUAGE" : "all"
      }
    },
    {
      "name" : "en_US",
      "displayName" : "English",
      "description" : "English",
      "inherits" : "default",
      "cacheVariables" : {
        "LANGUAGE" : "en_US"
      }
    },
    {
      "name" : "zh_CN",
      "displayName" : "Simplified Chinese",
      "description" : "简体中文",
      "inherits" : "default",
      "cacheVariables" : {
        "LANGUAGE" : "zh_CN"
      }
    },
    {
      "name" : "zh_TW",
      "displayName" : "Traditional Chinese",
      "description" : "繁體中文",
      "inherits" : "default",
      "cacheVariables" : {
        "LANGUAGE" : "zh_TW"
      }
    }
  ]
}
